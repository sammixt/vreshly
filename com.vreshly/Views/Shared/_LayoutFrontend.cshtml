    <script type="text/javascript">function toast(type, message) {
            toastr[type](`👋 ${message}`, `${capitalize(type)}!`, {
                closeButton: true,
                tapToDismiss: true,
                rtl: false
            });
        }

        function capitalize(word) {
            const lower = word.toLowerCase();
            return word.charAt(0).toUpperCase() + lower.slice(1);
        }</script>

